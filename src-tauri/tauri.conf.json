{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "fapptap-ui",
  "version": "0.1.0",
  "identifier": "com.pornaid.fapptap",
  "build": {
    "beforeDevCommand": "",
    "devUrl": "file:///C:/Files/Projects/fapptap/dist/index.html",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "fapptap-ui",
        "width": 1024,
        "height": 768
      }
    ],
    "security": {
      "csp": {
        "default-src": ["'self'"],
        "img-src": ["'self'", "asset:", "https://asset.localhost", "data:"],
        "script-src": ["'self'", "'unsafe-inline'", "'unsafe-eval'"],
        "style-src": ["'self'", "'unsafe-inline'"]
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "store": {},
    "shell": {
      "open": true
    },
    "dialog": null,
    "fs": {
      "scope": ["**"]
    },
    "opener": {
      "requireLiteralLeadingDot": false
    }
  }
}
